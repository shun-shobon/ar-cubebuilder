{
  "name": "ar-cubebuilder",
  "version": "0.0.0",
  "description": "AR sandbox game like Minecraft.",
  "homepage": "https://github.com/shun-shobon/ar-cubebuilder#readme",
  "bugs": "https://github.com/shun-shobon/ar-cubebuilder/issues",
  "license": "MIT",
  "author": "Shuntaro Nishizawa <me@shun.technology> (https://github.com/shun-shobon)",
  "repository": "https://github.com/shun-shobon/ar-cubebuilder.git",
  "scripts": {
    "build": "webpack",
    "clean": "rimraf dist",
    "dev": "webpack serve",
    "lint": "run-p eslint stylelint",
    "eslint": "eslint --ignore-path .gitignore 'src/**/*.{tsx,ts}'",
    "eslint:fix": "eslint --ignore-path .gitignore 'src/**/*.{tsx,ts}' --fix",
    "stylelint": "stylelint --ignore-path .gitignore 'src/**/*.{scss,css}'",
    "stylelint:fix": "stylelint --ignore-path .gitignore 'src/**/*.{scss,css}' --fix"
  },
  "lint-staged": {
    "**/*.{tsx,ts}": "eslint --fix",
    "**/*.{scss,css}": "stylelint --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "core-js": "^3.7.0",
    "preact": "^10.5.7",
    "regenerator-runtime": "^0.13.7",
    "tslib": "^2.0.3"
  },
  "devDependencies": {
    "@approvers/stylelint-config-scss": "^1.0.0",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@types/copy-webpack-plugin": "^6.2.0",
    "@types/fibers": "^3.1.0",
    "@types/html-webpack-plugin": "^3.2.4",
    "@types/node": "^14.14.7",
    "@types/sass": "^1.16.0",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "autoprefixer": "^10.0.2",
    "babel-loader": "^8.2.1",
    "copy-webpack-plugin": "^6.3.1",
    "css-loader": "^5.0.1",
    "cssnano": "^4.1.10",
    "dotenv": "^8.2.0",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "fibers": "^5.0.0",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.1.7",
    "postcss-loader": "^4.0.4",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "sass": "^1.29.0",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "stylelint": "^13.7.2",
    "stylelint-config-recess-order": "^2.3.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.18.0",
    "ts-loader": "^8.0.11",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  },
  "private": true
}
